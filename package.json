{
  "name": "z-ai-acp",
  "version": "0.0.33",
  "description": "An ACP-compatible coding agent powered by Claude Code SDK with Z.AI integration",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "npm run build && npm run start",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "npm run lint && npm run format:check",
    "test": "vitest",
    "test:integration": "RUN_INTEGRATION_TESTS=true vitest run",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "homepage": "https://github.com/softkr/z-ai-acp#readme",
  "bugs": {
    "url": "https://github.com/softkr/z-ai-acp/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/softkr/z-ai-acp.git"
  },
  "author": "Z.AI",
  "license": "Apache-2.0",
  "dependencies": {
    "@agentclientprotocol/sdk": "0.12.0",
    "@anthropic-ai/claude-agent-sdk": "0.1.76",
    "@anthropic-ai/claude-code": "2.0.76",
    "@modelcontextprotocol/sdk": "1.25.1",
    "diff": "8.0.2",
    "uuid": "13.0.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "0.71.2",
    "@types/node": "25.0.3",
    "@types/uuid": "11.0.0",
    "@typescript-eslint/eslint-plugin": "8.50.1",
    "@typescript-eslint/parser": "8.50.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "globals": "16.5.0",
    "prettier": "3.7.4",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  }
}