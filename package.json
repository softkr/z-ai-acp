{
  "name": "z-ai-acp",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.11.0",
  "description": "An ACP-compatible coding agent powered by Claude Code SDK with Z.AI integration (TypeScript)",
  "main": "dist/lib.js",
  "bin": {
    "z-ai-acp": "./dist/index.js"
  },
  "type": "module",
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "tsc",
    "bundle": "esbuild src/index.ts --bundle --platform=node --target=node18 --format=cjs --outfile=dist/bundle.cjs --packages=bundle",
    "build:binaries": "yarn bundle && pkg dist/bundle.cjs --compress Brotli --targets node18-macos-arm64,node18-macos-x64,node18-linux-x64,node18-win-x64 --output bin/z-ai-acp",
    "start": "node dist/index.js",
    "dev": "npm run build && npm run start",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "npm run lint && npm run format:check",
    "test": "vitest",
    "test:integration": "RUN_INTEGRATION_TESTS=true vitest run",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "claude",
    "acp",
    "agent",
    "anthropic",
    "typescript",
    "sdk",
    "code",
    "z-ai",
    "glm",
    "integration"
  ],
  "homepage": "https://github.com/z-ai/z-ai-acp#readme",
  "bugs": {
    "url": "https://github.com/z-ai/z-ai-acp/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z-ai/z-ai-acp.git"
  },
  "author": "Z.AI",
  "license": "Apache-2.0",
  "dependencies": {
    "@agentclientprotocol/sdk": "0.7.0",
    "@anthropic-ai/claude-agent-sdk": "0.1.55",
    "@anthropic-ai/claude-code": "2.0.55",
    "@modelcontextprotocol/sdk": "1.23.0",
    "diff": "8.0.2",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "0.71.0",
    "@types/node": "24.10.1",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.48.0",
    "@typescript-eslint/parser": "8.48.0",
    "esbuild": "^0.27.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "globals": "16.5.0",
    "pkg": "^5.8.1",
    "prettier": "3.7.3",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "vitest": "4.0.14"
  },
  "pkg": {}
}
